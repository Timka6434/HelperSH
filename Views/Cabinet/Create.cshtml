@model test_indentity.Models.CabinetList

@{
    ViewData["Title"] = "Создать кабинет";
}

<h2>Создать кабинет</h2>

<form asp-action="Create" method="post" >
    <div class="form-group">
        <label asp-for="CabinetName">Наименование кабинета</label>
        <input asp-for="CabinetName" class="form-control" />
        <span asp-validation-for="CabinetName" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="CabinetNumber">Номер кабинета</label>
        <input asp-for="CabinetNumber" class="form-control" />
        <span asp-validation-for="CabinetNumber" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary mt-3">Создать</button>
    <a asp-action="Index" class="btn btn-secondary mt-2">Отмена</a>
</form>
